{
  "name": "start-your-engines",
  "version": "1.0.0",
  "description": "control the apps you want to start",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninjacoder88/start-your-engines.git"
  },
  "author": "ninja software consulting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninjacoder88/start-your-engines/issues"
  },
  "homepage": "https://github.com/ninjacoder88/start-your-engines#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^18.0.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "requirejs": "^2.3.6",
    "save": "^2.4.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "start_your_engines"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
